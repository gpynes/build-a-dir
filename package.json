{
    "name": "build-a-dir",
    "displayName": "build-a-dir",
    "description": "A test ext to build directories for you",
    "version": "0.0.1",
    "publisher": "idunno",
    "engines": {
        "vscode": "^1.26.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:extension.sayHello",
        "onCommand:extension.buildDir"
    ],
    "main": "./out/extension",
    "contributes": {
        "commands": [
            {
                "command": "extension.sayHello",
                "title": "Hello World"
            },
            {
                "command": "extension.buildDir",
                "title": "Build A Directory"
            }
        ],
        "menus": {
            "editor/context": [
				{
					"command": "extension.sayHello",
					"group": "mzdev"
				},
				{
					"command": "extension.buildDir",
					"group": "1_modification"
				}
			],
			"editor/title": [
				{
					"command": "extension.sayHello",
					"group": "mzdev"
				},
				{
					"command": "extension.buildDir",
					"group": "1_modification"
				}
			],
			"explorer/context": [
				{
					"command": "extension.sayHello",
					"group": "mzdev"
				},
				{
					"command": "extension.buildDir",
					"group": "1_modification"
				}
			]
        }
        
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "npm run compile && node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.6.1",
        "vscode": "^1.1.21",
        "@types/node": "^8.10.25",
        "@types/mocha": "^2.2.42"
    }
}
